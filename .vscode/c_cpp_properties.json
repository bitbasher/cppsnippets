{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.26100.0",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-msvc-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": [
                "build/compile_commands.json"
            ],
            "recursiveIncludes": {
                "reduce": "never"
            },
            "browse": {
                "databaseFilename": ".vscode/.vscode_browse.vc.db"
            }
        }
    ],
    "version": 4
}