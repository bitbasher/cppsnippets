{
    "cmake.configureOnOpen": true,
    "cmake.generator": "Visual Studio 17 2022",
    "cmake.buildType": "Debug",
    "cmake.ctest.enabled": false,
    "cmake.variants": {
        "buildType": {
            "default": "debug",
            "choices": {
                "debug": {
                    "short": "Debug",
                    "long": "Debug: Optimized for debugging",
                    "buildType": "Debug"
                },
                "release": {
                    "short": "Release",
                    "long": "Release: Optimized for speed",
                    "buildType": "Release"
                }
            }
        }
    },
    "files.autoSave": "afterDelay",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.dimInactiveRegions": true,
    "terminal.integrated.shellIntegration.enabled": true,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.integratedConsole.startInBackground": false,
    "terminal.integrated.shellIntegration.showWelcome": false,
    "testMate.cpp.test.executables": "{build,Build,BUILD,out}/**/*{test,Test,TEST}*.{exe,out}",
    "testMate.cpp.discovery.testListCaching": false,
    "testMate.cpp.log.logpanel": true,
    "testMate.cpp.debug.configTemplate": {
        "type": "cppvsdbg",
        "linux": {
            "type": "cppdbg",
            "MIMode": "gdb"
        },
        "darwin": {
            "type": "cppdbg",
            "MIMode": "lldb"
        },
        "win32": {
            "type": "cppvsdbg"
        }
    },
    "json.schemas": [],
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "off",
            "other": "off"
        }
    },
    "markdown.preview.breaks": true,
    "files.exclude": {
        "**/.git": false,
        "**/.vscode": false,
        "**/node_modules": true,
        "**/.vs": true,
        "**/src-orig": true
    },
    "C_Cpp.files.exclude": {
        "**/src-orig/**": true,
        "**/vcpkg_installed/**": true,
        "**/.vs/**": true,
        "**/build/**": true
    },
    "files.associations": {
        "xstring": "cpp",
        "vector": "cpp",
        "filesystem": "cpp",
        "functional": "cpp",
        "initializer_list": "cpp",
        "algorithm": "cpp",
        "any": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "deque": "cpp",
        "exception": "cpp",
        "coroutine": "cpp",
        "resumable": "cpp",
        "format": "cpp",
        "forward_list": "cpp",
        "fstream": "cpp",
        "future": "cpp",
        "iomanip": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "list": "cpp",
        "locale": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "ostream": "cpp",
        "ranges": "cpp",
        "ratio": "cpp",
        "regex": "cpp",
        "set": "cpp",
        "span": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "stdfloat": "cpp",
        "stop_token": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "utility": "cpp",
        "variant": "cpp",
        "xfacet": "cpp",
        "xhash": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocbuf": "cpp",
        "xlocinfo": "cpp",
        "xlocmes": "cpp",
        "xlocmon": "cpp",
        "xlocnum": "cpp",
        "xloctime": "cpp",
        "xmemory": "cpp",
        "xtr1common": "cpp",
        "xtree": "cpp",
        "xutility": "cpp",
        "qtabwidget": "cpp",
